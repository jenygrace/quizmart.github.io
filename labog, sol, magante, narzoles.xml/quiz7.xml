<quiz>
  <question>
    <text>Question 1: What does GUI stand for?</text>
    <options>
      <option>Graphical User Interface</option>
      <option>General User Interface</option>
      <option>Graphical User Interaction</option>
      <option>General User Interaction</option>
    </options>
    <answer>Graphical User Interface</answer>
  </question>
  <question>
